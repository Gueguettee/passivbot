# Meta-Optimizer Deployment Configuration
# Copy this file to .env and fill in your values

# Remote instance SSH connection
REMOTE_HOST=ubuntu@your-instance-ip.com
SSH_KEY=~/.ssh/your-private-key.pem

# Remote directory (where passivbot will be deployed)
REMOTE_DIR=/home/ubuntu/passivbot

# Python command on remote
PYTHON_CMD=python3

# Virtual environment name
VENV_NAME=venv
